module Module_recon_ext_horizontal_ips_01;
@RSAAlert(identifiers={"ip_src"})

SELECT * FROM Event (device_type = 'ciscoasa' AND event_cat_name = 'User.Activity.Privileged Use.Successful' AND ip_src IS NOT NULL AND ip_src NOT IN ('127.0.0.1', '244.0.0.252', '0.0.0.0') AND (net_src NOT IN ('amex_internal_src', 'amex_public_src') OR net_src IS NULL)).std:unique(ip_src) OUTPUT first every 5 minutes;
