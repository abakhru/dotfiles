#Install SUNWappserver 2.1 only no other version
mkdir /etc/jiss
cp ./jiss.conf /opt/sun/comms/jiss/etc/
ln -s /opt/sun/comms/jiss/etc/jiss.conf /etc/jiss/
ln -s /opt/sun/comms/jiss/etc/jiss_passwd.conf /etc/jiss/
/opt/sun/comms/jiss/bin/setup.sh -v -f /etc/jiss/jiss.conf -t all

#stop-all
/opt/sun/comms/messaging64/bin/stop-msg
/opt/sun/comms/jiss/bin/svc_control.sh stop
/opt/SUNWappserver/bin/asadmin stop-domain domain1
/etc/init.d/imq stop
sleep 3;
/opt/sun/dsee7/bin/dsadm stop /var/opt/sun/dsins1

#start-all
/opt/sun/dsee7/bin/dsadm start /var/opt/sun/dsins1
/opt/sun/comms/messaging64/bin/start-msg
/opt/SUNWappserver/bin/asadmin start-domain domain1
/etc/init.d/imq start
/opt/sun/comms/jiss/bin/svc_control.sh start

