
#cd  /net/tornedo/orion_builds/orion2/m2/b6/sparc/Solaris_sparc/Product


find . -name IPL* -exec basename {} \; >> /Helpers_X86/packagelist1.txt

find . -name SUNW* -exec basename {} \; >> /Helpers_X86/packagelist2.txt

sort -u /Helpers_X86/packagelist1.txt >> /Helpers_X86/Packagelist1.txt
sort -u /Helpers_X86/packagelist2.txt >> /Helpers_X86/Packagelist2.txt

rm /Helpers_X86/Complete_Clean/packagelist

sort /Helpers_X86/Packagelist1.txt /Helpers_X86/Packagelist2.txt >> /Helpers_X86/Complete_Clean/packagelist

 
rm -rf /Helpers_X86/Pack*
rm -rf /Helpers_X86/pack*
